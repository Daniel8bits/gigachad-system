{
    "compilerOptions": {
      "target": "ES2020",
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "noImplicitAny": false,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "module": "CommonJS",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true
    },
    "include": [
      "src"
    ]
  }
  